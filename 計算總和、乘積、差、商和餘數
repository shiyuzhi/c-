#include <iostream>  
using namespace std;  
  
int main()  
{  
    char op;  
    cin >> op;  
  
    int a, b;  
    cin >> a >> b;  
    if ((a > 1000 && b > 1000) && (a > 0 && b > 0)) return 0;  
  
    switch (op)   
    {  
    case '+':  
      cout << a << "+" << b << "=" << a + b << endl;  
      break;  
    case '-':  
      cout << a << "-" << b << "=" << a - b << endl;  
      break;  
    case '*':  
      cout << a << "*" << b << "=" << a * b << endl;  
      break;  
    case '/':   
        int r = a % b;  
        if (r >= 0)  
        cout << a << "/" << b << "=" << a / b << "..." << r << endl;  
        break;  
  
    }  
  
    return 0;  
      
}  
